# Fibonacci sequence
func fun{ 
	if(x < 2){  return(1);  }
	y = 0;
	x = x - 1;
	y = fun(x);
	x = x - 1;
	return(y + fun(x));
};

x=6;
if( x > 5 )
	print( x );		# output a number
print(fun(x));

